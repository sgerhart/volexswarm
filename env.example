# VolexSwarm Environment Variables
# Copy this file to .env and update with your values

# Vault Configuration
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=root

# Database Configuration (optional - can be stored in Vault)
DB_HOST=db
DB_PORT=5432
DB_NAME=volextrades
DB_USER=volex
DB_PASSWORD=volex_pass

# Agent Configuration (optional - can be stored in Vault)
RESEARCH_AGENT_PORT=8001
SIGNAL_AGENT_PORT=8002
STRATEGY_AGENT_PORT=8003
EXECUTION_AGENT_PORT=8004
RISK_AGENT_PORT=8005
MONITOR_AGENT_PORT=8006
OPTIMIZE_AGENT_PORT=8007
META_AGENT_PORT=8008
COMPLIANCE_AGENT_PORT=8009
BACKTEST_AGENT_PORT=8010

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/volexswarm.log

# Development Configuration
DEBUG=false
ENVIRONMENT=development 