# VolexSwarm Environment Variables
# Copy this file to .env and fill in your actual values

# Vault Configuration
# Run: python scripts/setup/init_vault_production.py to generate this token
VAULT_ROOT_TOKEN=your_vault_root_token_here

# Database Configuration (optional overrides)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=volextrades
# DB_USER=volex
# DB_PASSWORD=volex_pass

# OpenAI Configuration (stored in Vault)
# OPENAI_API_KEY=your_openai_api_key_here

# Exchange API Keys (stored in Vault)
# BINANCE_API_KEY=your_binance_api_key_here
# BINANCE_SECRET_KEY=your_binance_secret_key_here
